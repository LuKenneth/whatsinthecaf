  target 'WhatsInTheCafTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WhatsInTheCafUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'WhatsInTheCafIntents' do
   use_frameworks!
      pod 'mailcore2-ios'
      pod 'Firebase'
      pod 'Firebase/Database'
      pod 'Firebase/Auth'
      # Pods for testing
  end



end
